# Database connection (PostgreSQL with TimescaleDB)
DATABASE_URL=postgresql://user:password@localhost:5432/qtos_data

# Finnhub API key (for news and insider data)
FINNHUB_API_KEY=

# Ingestion schedule (cron or interval)
PRICE_INGESTION_INTERVAL_MINUTES=5
NEWS_INGESTION_INTERVAL_MINUTES=60
INSIDER_INGESTION_INTERVAL_MINUTES=60

# Symbols to track (comma-separated, or leave empty to track all from agents)
TRACKED_SYMBOLS=SPY,QQQ,TLT,AAPL,MSFT,GOOGL

# API server
API_HOST=0.0.0.0
API_PORT=8001
